{
  "packageId": "Cmf.Custom.HTML",
  "version": "1.3.0",
  "description": "Cmf Custom AMSOsram Cmf.Custom.HTML Package",
  "packageType": "Html",
  "isInstallable": true,
  "isUniqueInstall": false, 
  "contentToPack": [
    {
      "source": "src/packages/*",
      "target": "node_modules",
      "ignoreFiles": [
        ".npmignore"
      ]
    },
    {
      "source": "apps/customization.web/node_modules/@criticalmanufacturing/mes-ui-web/bundles",
      "target": ""
    },
	  {
      "source": "../Libs/LBOs/TypeScript/APIReference*",
      "target": "node_modules/cmf.lbos"
    },
    {
      "source": "../Libs/LBOs/TypeScript/cmf.lbos.*",
      "target": "node_modules/cmf.lbos"
    },
    {
      "source": "../Libs/LBOs/TypeScript/package.json",
      "target": "node_modules/cmf.lbos"
    },
	{
      "source": "config.json",
      "action": "Transform"
    }
  ]
}