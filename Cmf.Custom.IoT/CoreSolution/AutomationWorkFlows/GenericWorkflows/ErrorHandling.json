		{
			"id": "task_13642",
			"reference": {
				"name": "codeExecution",
				"package": {
					"name": "@criticalmanufacturing/connect-iot-controller-engine-core-tasks",
					"version": "8.3.3-202201212"
				}
			},
			"settings": {
				"tsCode": [
					"import { Framework } from 'framework';",
					"",
					"export default class {",
					"",
					"    /** Allows accessing external functions */",
					"    private framework: Framework;",
					"",
					"    constructor(framework: Framework) {",
					"        this.framework = framework;",
					"    }",
					"",
					"    /*",
					"     * Entry point of the class (IMPORTANT: don't change the signature of this method)",
					"     * Should return an object containing the values for each output to emit",
					"     * If necessary, use the parameter \"outputs\" to emit data while running the code.",
					"     */",
					"    public async main(inputs: any, outputs: any): Promise<any> {",
					"",
					"        let errorText;",
					"        let errorCode;",
					"        ",
					"        // get error object string",
					"        if (inputs.Error) {",
					"          errorText = inputs.Error.toString();",
					"        }",
					"        ",
					"        // get error code if available",
					"        if(inputs.ErrorCode) {",
					"            errorCode = inputs.ErrorCode;",
					"        }",
					"        if (!errorCode && inputs.Error) {",
					"            const parsedError = inputs.Error.toString().split(\":\",3);",
					"            if(parsedError.length == 3) {",
					"                errorCode = `${parsedError[0]}:${parsedError[1]}`;",
					"            }",
					"            else {",
					"                errorCode = \"Unknown\";",
					"            }             ",
					"        }",
					"        if (!errorCode && inputs.ErrorText) {",
					"            const parsedError = inputs.Error.toString().split(\":\", 3);",
					"            if (parsedError.length == 3) {",
					"                errorCode = `${parsedError[0]}:${parsedError[1]}`;",
					"            }",
					"            else {",
					"                errorCode = \"Unknown\";",
					"            }",
					"        }",
					"        if(!errorCode) {",
					"            errorCode = \"Unknown\";",
					"        }",
					"        ",
					"        // get resource name ",
					"        let resourceName = inputs.ResourceName;",
					"        if (!resourceName) {",
					"            resourceName = this.framework.dataStore.retrieve(\"Generic_ResourceName\", null);",
					"        }",
					"",
					"        // compose error text",
					"        if (!errorText) {",
					"            errorText = `${inputs.ErrorCode} [${resourceName}] - ${inputs.ErrorText}`;",
					"        }",
					"        ",
					"        // get material name if available",
					"        let materialName;",
					"        if(inputs.MaterialName) {",
					"            materialName = inputs.MaterialName;",
					"        }",
					"        if (!materialName && inputs.MaterialData) {",
					"            materialName = inputs.MaterialData.MaterialName;",
					"        }",
					"",
					"        // get container name if available",
					"        let containerName;",
					"        if (inputs.ContainerName) {",
					"            containerName = inputs.ContainerName;",
					"        }",
					"        if (!containerName && inputs.ContainerData) {",
					"            containerName = inputs.ContainerData.ContainerName;",
					"        }",
					"        if (!containerName && inputs.MaterialData) {",
					"            containerName = inputs.MaterialData.ContainerName;",
					"        }",
					"",
					"        // logging error",
					"        this.framework.logger.error(errorText);",
					"",
					"        if (errorCode.startsWith(\"MaterialArrivedError\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                NoAdditionalAction: true",
					"            };",
					"        } else if (errorCode.startsWith(\"CarrierIdFailedToRead\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                 NoAdditionalAction: true",
					"            };",
					"        } else if (errorCode.startsWith(\"CarrierFailedToClamp\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                NoAdditionalAction: true",
					"                // trigger will be sent here",
					"                // TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"InvalidCarrierDocked\")) {",
					"            // hold will be trigger by MES on Docking immediattelly, by container",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText)",
					"            };",
					"        } else if (errorCode.startsWith(\"CarrierFailedToOpen\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"CassetteSlotMapFailed\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"CassetteFailedToLoad\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"TrackInValidationFailed\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                NoAdditionalAction: true",
					"            };",
					"        }",
					"        else if (errorCode.startsWith(\"RecipeValidationFailed\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                NoAdditionalAction: true",
					"            };",
					"        }",
					"        else if (errorCode.startsWith(\"FailingTrackIn\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                NoAdditionalAction: true",
					"            };",
					"        } else if (errorCode.startsWith(\"TrackInFailed\")) {",
					"            return { Message: errorText, ",
					"                     ResourceName: resourceName, ",
					"                     Error: new Error(errorText), ",
					"                     ContainerName: containerName,",
					"                     SetMaterialOnContainerOnHold: true,",
					"                     TriggerHold: true };",
					"        } else if (errorCode.startsWith(\"PPSelectFailed\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                MaterialName: materialName,",
					"                TriggerAbort: true",
					"            };",
					"        } else if (errorCode.startsWith(\"StartCommandFailed\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                MaterialName: materialName,",
					"                TriggerAbort: true",
					"            };",
					"        } else if (errorCode.startsWith(\"LotProcessError\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                MaterialName: materialName,",
					"                TriggerFutureHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"FatalLotProcessError\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                MaterialName: materialName,",
					"                TriggerAbort: true",
					"            };",
					"        } else if (errorCode.startsWith(\"WaferProcessError\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                MaterialName: materialName,",
					"                TriggerFutureHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"FatalWaferProcessError\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                MaterialName: materialName,",
					"                TriggerAbort: true",
					"            };",
					"        } else if (errorCode.startsWith(\"CarrierFailedToClose\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"CassetteFailedToUnload\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"CarrierFailedToUnclamp\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"FailedOnReadyToUnload\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"MaterialRemovedError\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                ContainerName: containerName,",
					"                TriggerHold: true",
					"            };",
					"        } else if (errorCode.startsWith(\"OtherError\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText)                ",
					"            };",
					"        } else if (errorCode.startsWith(\"NotifyErrorOnly\")) {",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                NoAdditionalAction: true",
					"            };",
					"        } else {",
					"            // unknown error code, just send notification",
					"            return {",
					"                Message: errorText,",
					"                ResourceName: resourceName,",
					"                Error: new Error(errorText),",
					"                NoAdditionalAction: true",
					"            };",
					"        }       ",
					"      ",
					"    }",
					"}",
					""
				],
