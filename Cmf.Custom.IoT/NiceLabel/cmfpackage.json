{
  "packageId": "Cmf.Custom.IoT.NiceLabel",
  "version": "1.2.0",
  "description": "Cmf Custom Nice Label Printer Integration Data Package",
  "packageType": "IoTData",
  "isInstallable": true,
  "isUniqueInstall": true,
  "steps": [
    {
      "order": 1,
      "type": "Generic",
      "onExecute": "$(Agent.Root)/agent/scripts/stop_host.ps1"
    },
    {
      "order": 2,
      "type": "TransformFile",
      "file": "Cmf.Foundation.Services.HostService.dll.config",
      "tagFile": true
    },
    {
      "order": 3,
      "type": "Generic",
      "onExecute": "$(Agent.Root)/agent/scripts/start_host.ps1"
    },
    {
      "order": 4,
      "type": "Generic",
      "onExecute": "$(Package[Cmf.Custom.Package].TargetDirectory)/GenerateLBOs.ps1"
    }
  ],
  "contentToPack": [
    {
      "source": "MasterData/*",
      "target": "MasterData/$(version)",
      "contentType": "MasterData"
    },
    {
      "source": "AutomationWorkFlows/*",
      "target": "AutomationWorkFlows",
      "contentType": "AutomationWorkFlows"
    }
  ]
}