{
  "packageId": "Cmf.Custom.IoT.Data",
  "version": "1.0.0",
  "description": "Cmf Custom AMSOsram Cmf.Custom.IoT.Data Package",
  "packageType": "IoTData",
  "isInstallable": true,
  "isUniqueInstall": true,
  "contentToPack": [
    {
      "source": "$(version)/*",
      "target": "MasterData/$(version)",
      "contentType": "MasterData"
    },
    {
      "source": "AutomationWorkFlows/*",
      "target": "AutomationWorkFlows",
      "ignoreFiles": [
        "../.cmfpackageignore"
      ],
      "contentType": "AutomationWorkFlows"
    }
  ]
}