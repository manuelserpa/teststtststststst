					"import { Framework } from 'framework';",
					"",
					"export default class {",
					"",
					"    /** Allows accessing external functions */",
					"    private framework: Framework;",
					"",
					"    constructor(framework: Framework) {",
					"        this.framework = framework;",
					"    }",
					"",
					"    /*",
					"     * Entry point of the class (IMPORTANT: don't change the signature of this method)",
					"     * Should return an object containing the values for each output to emit",
					"     * If necessary, use the parameter \"outputs\" to emit data while running the code.",
					"     */",
					"    public async main(inputs: any, outputs: any): Promise<any> {",
					"        // Add code here",
					"        const materialData = inputs.MaterialData[0];",
					"        const PortID = materialData.LoadPortPosition;",
					"        const WaferNum = materialData.LoadPortPosition;",
					"        const LotID = materialData.LoadPortPosition;",
					"        const ProductID = materialData.LoadPortPosition;",
					"        const StepID = materialData.LoadPortPosition;",
					"        const RecipeID = materialData.LoadPortPosition;",
					"        const SlotID = materialData.Recipe.NameOnEquipment;",
					"        const WaferID = materialData.Recipe.NameOnEquipment;",
					"",
					"        return {",
					"            PORT_ID: PortID,",
					"            WAFERS_NUM: WaferNum,",
					"            LOT_ID: LotID,",
					"            PRODUCT_ID: ProductID,",
					"            STEP_ID: StepID,",
					"            RECIPE_ID: RecipeID,",
					"            SLOT_ID: SlotID,",
					"            WAFER_ID: WaferID",
					"        };",
					"    }",
					"}",
					""
				],
