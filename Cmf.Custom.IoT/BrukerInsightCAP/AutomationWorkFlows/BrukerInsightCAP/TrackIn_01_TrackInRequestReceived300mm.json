		{
			"id": "task_116330",
			"reference": {
				"name": "codeExecution",
				"package": {
					"name": "@criticalmanufacturing/connect-iot-controller-engine-core-tasks",
					"version": "9.1.0-202209072"
				}
			},
			"settings": {
				"tsCode": [
					"import { Framework } from 'framework';",
					"",
					"export default class {",
					"",
					"    /** Allows accessing external functions */",
					"    private framework: Framework;",
					"",
					"    constructor(framework: Framework) {",
					"        this.framework = framework;",
					"    }",
					"",
					"    /*",
					"     * Entry point of the class (IMPORTANT: don't change the signature of this method)",
					"     * Should return an object containing the values for each output to emit",
					"     * If necessary, use the parameter \"outputs\" to emit data while running the code.",
					"     */",
					"    public async main(inputs: any, outputs: any): Promise<any> {",
					"        // Add code here",
					"        const capacity = 13;",
					"        const materialData = inputs.MaterialData[0];",
					"        var container = inputs.container;",
					"        let commandParameters: any[] = [];",
					"        var substarteCount = 0;",
					"",
					"        if (container.SlotMap == null){         ",
					"            throw new Error(\"SlotMap cannot be empty\");",
					"        }",
					"",
					"        var slotMapList = container.SlotMap.split(\"\");",
					"",
					"        commandParameters.push({",
					"            type: \"L\", value: [",
					"                { type: \"A\", value: \"Capacity\" },",
					"                { type: \"U1\", value: capacity }",
					"            ]",
					"        });",
					"",
					"",
					"        let waferParameters: any[] = [];",
					"        let slotMap: any[] = [];",
					"",
					"        commandParameters.push({",
					"            type: \"L\", value: [",
					"                { type: \"A\", value: \"ContentMap\" },",
					"                { type: \"L\", value: waferParameters }",
					"            ]",
					"        });",
					"",
					"        for (let i = 0; i <= capacity; i++) {",
					"            let subMaterialInSlot = (<any[]>materialData.SubMaterials).find(subMaterial => subMaterial.Slot == i)",
					"            if (subMaterialInSlot != null) {",
					"                waferParameters.push({",
					"                    type: \"L\", value: [",
					"                        { type: \"A\", value: materialData.MaterialName },",
					"                        { type: \"A\", value: subMaterialInSlot.MaterialName }",
					"                    ]",
					"                });",
					"            }",
					"            else {",
					"                waferParameters.push({",
					"                    type: \"L\", value: [",
					"                        { type: \"A\", value: \"\" },",
					"                        { type: \"A\", value: \"\" }",
					"                    ]",
					"                });",
					"            }",
					"",
					"            let slot = slotMapList[i];",
					"            if(slot === \"1\"){",
					"                slotMap[i] = { type: \"U1\", value: 3 };",
					"            } else {",
					"                slotMap[i] = { type: \"U1\", value: 1 };",
					"            }",
					"        }",
					"        ",
					"        commandParameters.push({",
					"            type: \"L\", value: [",
					"                { type: \"A\", value: \"SlotMap\" },",
					"                { type: \"L\", value: slotMap }",
					"            ]",
					"        });",
					"",
					"        commandParameters.push({",
					"            type: \"L\", value: [",
					"                { type: \"A\", value: \"SubstrateCount\" },",
					"                { type: \"U1\", value: substarteCount }",
					"            ]",
					"        });",
					"",
					"        commandParameters.push({",
					"            type: \"L\", value: [",
					"                { type: \"A\", value: \"Usage\" },",
					"                { type: \"A\", value: \"PRODUCT\" }",
					"            ]",
					"        });",
					"",
					"",
					"        // emit output during execution: outputs.output1.emit(\"something\");",
					"        // return example: return { output1: inputs.input1, output2: \"Hello World\" };",
					"",
					"        return {",
					"            ContainerID: container.ContainerName,",
					"            LoadPort: container.LoadPortPosition,",
					"            CarrierAttributes: commandParameters",
					"        }",
					"    }",
					"}",
					""
				],
