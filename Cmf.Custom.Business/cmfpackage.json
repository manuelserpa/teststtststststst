{
  "packageId": "Cmf.Custom.Business",
  "version": "1.12.0",
  "description": "Cmf Custom AMSOsram Cmf.Custom.Business Package",
  "packageType": "Business",
  "isInstallable": true,
  "isUniqueInstall": false,
  "steps": [
    {
      "order": 1,
      "type": "Generic",
      "onExecute": "$(Agent.Root)/agent/scripts/stop_host.ps1"
    },
    {
      "order": 2,
      "type": "DeployFiles",
      "contentPath": "**/!(Cmf.Custom.*.BusinessObjects*).dll"
    },
    {
      "order": 3,
      "type": "Generic",
      "onExecute": "$(Agent.Root)/agent/scripts/start_host.ps1"
    }
  ],
  "contentToPack": [
    {
      "source": "Release/*.dll",
      "target": ""
    }
  ]
}