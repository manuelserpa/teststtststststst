{
    "packageId": "Cmf.Custom.TibcoEMS",
    "version": "1.0.0",
    "description": "Cmf TibcoEMS Gateway Package",
    "packageType": "Generic",
	"targetDirectory": "TibcoEMSGateway",
    "isInstallable": true,
    "isUniqueInstall": false,
    "dependencies": [],
	"buildSteps": [
		{
			"command": "dotnet",
			"args": [
				"build -c Release"
			],
			"workingDirectory": "."
		}
	],	
    "contentToPack": [
        {
          "source": "Release/netcoreapp3.1/*.*",
          "target": ""
        }
      ],
	"steps": [
		{
			"order": "2",
			"type": "DeployFiles",
			"ContentPath": "**/**"
		}
	]
}
