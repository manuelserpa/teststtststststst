<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appSettings>

        <!-- CMF.HostService -->

        <add key="ApplicationName" value="TibcoEMSGateway"/>
        <add key="ClientTenantName" value="AMSOsram"/>
        <add key="HostAddress" value="{MESHostFullAddressName}"/>
        <add key="HostPort" value="{MESHostPort}"/>
        <add key="UseSSL" value="false"/>
        <add key="UserName" value="{MESHostUsername}"/>
        <add key="Password" value="{MESHostPassword}"/>
        <add key="SecurityAccessToken" value="{MESMessageBusSecurityToken}"/>
        <add key="ClientSettingsProvider.ServiceUri" value=""/>

        <!-- CMF.MessageBus -->

        <!-- MessageBus.Host: Where Message Bus belong-->
        <add key="MessageBus.Host" value="{MESMessageBusFullAddressName}"/>
        <add key="MessageBus.Port" value="{MESMessageBusPort}"/>
        <add key="MessageBus.SecurityToken" value="{MESMessageBusSecurityToken}"/>

        <!-- MessageBus.ExternalAddress: MES Message Bus to listen to -->
        <add key="MessageBus.ExternalAddress" value="wss://{MESMessageBusFullAddressName}:{MESHostPort}/messagebus"/>

        <!-- CMF.MessageBus.UseLoadBalancing: 
        /** FOR FUTURE USE IF NEEDED **/

        <add key="MessageBus.UseLoadBalancing" value ="false"/>
        <add key="MessageBus.LoadBalancing.Host" value =""/>
